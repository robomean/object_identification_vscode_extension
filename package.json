{
  "name": "mathobjectidentifier",
  "displayName": "MathObjectIdentifier",
  "description": "Properties of mathematical object from description",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mathobjectidentifier.processText",
    "onCommand:mathobjectidentifier.describeObject"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mathobjectidentifier.processText",
        "title": "Describe mathematical object"
      }
    ],
    "keybindings": [
      {
        "command": "mathobjectidentifier.describeObject",
        "key": "ctrl+enter",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "ChatGPT",
      "properties": {
        "chatGPT.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your OpenAI API key for accessing ChatGPT."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/tmp": "^0.2.6",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.3",
    "chatgpt": "^5.2.5",
    "openai": "^4.24.1",
    "tmp": "^0.2.3"
  }
}
